{
  "expo": {
    "name": "estoqueApp",
    "slug": "estoqueApp",
    "scheme": "acme",
    "plugins": [
      "expo-router",
      "expo-barcode-scanner"
    ],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "assetBundlePatterns": ["**/*"]
  }
}
